<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="TIBCO BW Oracle DataSource Factory">
   <implementation class="com.tibco.bw.jdbc.datasourcefactory.oracle.OracleDataSourceFactory"/>
   
   <property name="osgi.jdbc.driver.class" type="String" value="oracle.jdbc.OracleDriver"/>
   <property name="osgi.jdbc.driver.name" type="String" value="oracle"/>
   <property name="osgi.jdbc.driver.version" type="String" value="6"/>
   
   <service>
      <provide interface="org.osgi.service.jdbc.DataSourceFactory"/>
   </service>
</scr:component>
